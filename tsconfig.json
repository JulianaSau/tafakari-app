{
  "extends": "expo/tsconfig.base",
  "noImplicitAny": false,
  "compilerOptions": {
    "strictNullChecks": true,
    "strict": true,
    "module": "esnext",
    "jsx": "react-jsx",
    "paths": {
      "@/*": ["./src/*"]
    },
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*",
    "./App.tsx"
  ]
}
